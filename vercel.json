{
  "version": 2,
  "builds": [
    {
      "src": "app/index.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 60,
        "memory": 512
      }
    }
  ],
  "routes": [
    {
      "src": "/scrape",
      "dest": "app/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "app/index.py"
    }
  ]
}
